{
  "name": "tasar/laravel-sms",
  "description": "SMS package for laravel",
  "keywords": [
    "sms",
    "send-sms",
    "laravel",
    "laravel-6",
    "package"
  ],
  "homepage": "https://github.com/tasarweb/laravel-sms",
  "license": "MIT",
  "authors": [
    {
      "name": "Tasar",
      "email": "info@tasarweb.ir"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tasar\\SMS\\": "src"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload-dev": {
    "psr-4": {
      "Tasar\\SMS\\": "tests"
    }
  },
  "require": {
    "php": ">=7",
    "illuminate/support": "^5.5|^6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tasar\\SMS\\smsServiceProvider"
      ],
      "aliases": {
        "SMS": "Tasar\\SMS\\Facades"
      }
    }
  }
}
